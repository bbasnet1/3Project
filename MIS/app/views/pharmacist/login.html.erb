<div class="container entry-content">
  <div class="row">
    <div class="col-md-12 page-header">
<h1>Pharmacist#login</h1>
<p>Find me in app/views/pharmacist/login.html.erb</p>
    </div>
  </div>
  <div class="row login-body">
    <div class="col-md-6 login-content">
      <div style="width: 400px; height: 190px; box-shadow: 0px 0px 10px 0px #7D7D7D;">
      <%= link_to( image_tag("user-silhouette.png", :class=>"login-image", :alt=>"user silhouette")) %>
      <%= form_tag(:action => 'login_submit') do %>
          <p>USERNAME:</br><%= text_field_tag(:username) %></p>
          <%= submit_tag("Log In", :class=>'btn btn-primary')%>
      <% end %><br>
    </div>
  </div>
</div>
</div>
